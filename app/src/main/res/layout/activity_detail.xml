<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".ui.detail.DetailActivity">


    <View
        android:id="@+id/view"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="-8dp"
        android:layout_marginBottom="-8dp"
        android:background="@color/orange_1"
        app:layout_constraintBottom_toBottomOf="@+id/tv_calories_detail2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv_calories_detail"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/view2"
        android:layout_width="0.7dp"
        android:layout_height="0dp"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="32dp"
        android:background="@color/black_2"
        app:layout_constraintBottom_toTopOf="@+id/btn_add_to_diary_detail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view" />

    <TextView
        android:id="@+id/tv_calories_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/semi_bold"
        android:text="Calories"
        android:textColor="@color/black_3"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_image_detail" />

    <TextView
        android:id="@+id/tv_calories_detail2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/semi_bold"
        android:textColor="@color/black_2"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/tv_calories_detail"
        app:layout_constraintStart_toStartOf="@+id/tv_calories_detail"
        app:layout_constraintTop_toBottomOf="@+id/tv_calories_detail" />

    <TextView
        android:id="@+id/tv_name_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/bold"
        android:text="Food Name"
        android:textColor="@color/black_4"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_fat_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:fontFamily="@font/semi_bold"
        android:text="Fat"
        android:textColor="@color/black_3"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_fat_detail2"
        app:layout_constraintEnd_toStartOf="@+id/view2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view2" />

    <TextView
        android:id="@+id/tv_fat_detail2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/semi_bold"
        android:textColor="@color/black_2"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_cholestrol_detail"
        app:layout_constraintEnd_toEndOf="@+id/tv_fat_detail"
        app:layout_constraintStart_toStartOf="@+id/tv_fat_detail"
        app:layout_constraintTop_toBottomOf="@+id/tv_fat_detail" />

    <TextView
        android:id="@+id/tv_cholestrol_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="32dp"
        android:fontFamily="@font/semi_bold"
        android:text="Cholesterol"
        android:textColor="@color/black_3"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_cholestrol_detail2"
        app:layout_constraintEnd_toStartOf="@+id/view2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_fat_detail2" />

    <TextView
        android:id="@+id/tv_cholestrol_detail2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/semi_bold"
        android:textColor="@color/black_2"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_sugar_detail"
        app:layout_constraintEnd_toEndOf="@+id/tv_cholestrol_detail"
        app:layout_constraintStart_toStartOf="@+id/tv_cholestrol_detail"
        app:layout_constraintTop_toBottomOf="@+id/tv_cholestrol_detail" />

    <TextView
        android:id="@+id/tv_sugar_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="32dp"
        android:fontFamily="@font/semi_bold"
        android:text="Sugar"
        android:textColor="@color/black_3"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_sugar_detail2"
        app:layout_constraintEnd_toStartOf="@+id/view2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_cholestrol_detail2" />

    <TextView
        android:id="@+id/tv_sugar_detail2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/semi_bold"
        android:textColor="@color/black_2"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="@+id/tv_sugar_detail"
        app:layout_constraintStart_toStartOf="@+id/tv_sugar_detail"
        app:layout_constraintTop_toBottomOf="@+id/tv_sugar_detail" />

    <TextView
        android:id="@+id/tv_protein_detail2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:fontFamily="@font/semi_bold"
        android:textColor="@color/black_2"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_calcium_detail"
        app:layout_constraintEnd_toEndOf="@+id/tv_protein_detail"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/tv_protein_detail"
        app:layout_constraintTop_toBottomOf="@+id/tv_protein_detail" />

    <TextView
        android:id="@+id/tv_calcium_detail2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/semi_bold"
        android:textColor="@color/black_2"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_iron_detail"
        app:layout_constraintEnd_toEndOf="@+id/tv_calcium_detail"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/tv_calcium_detail"
        app:layout_constraintTop_toBottomOf="@+id/tv_calcium_detail" />

    <TextView
        android:id="@+id/tv_iron_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/semi_bold"
        android:text="Iron"
        android:textColor="@color/black_3"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_iron_detail2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/tv_calcium_detail2" />

    <TextView
        android:id="@+id/tv_calcium_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/semi_bold"
        android:text="Calcium"
        android:textColor="@color/black_3"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_calcium_detail2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/tv_protein_detail2" />

    <TextView
        android:id="@+id/tv_protein_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:fontFamily="@font/semi_bold"
        android:text="Protein"
        android:textColor="@color/black_3"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_protein_detail2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2" />

    <TextView
        android:id="@+id/tv_iron_detail2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/semi_bold"
        android:textColor="@color/black_2"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="@+id/tv_iron_detail"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_iron_detail"
        app:layout_constraintStart_toStartOf="@+id/tv_iron_detail"
        app:layout_constraintTop_toBottomOf="@+id/tv_iron_detail" />


    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/iv_image_detail"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/no_photo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_name_detail" />

    <Button
        android:id="@+id/btn_add_to_diary_detail"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/button"
        android:fontFamily="@font/bold"
        android:text="Add to diary"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />


    <ProgressBar
        android:id="@+id/pb_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>